            {
              "id": "SNYK-RHEL8-GLIBCMINIMALLANGPACK-5899345",
              "shortDescription": {
                "text": "High severity - Use After Free vulnerability in glibc-minimal-langpack"
              },
              "fullDescription": {
                "text": "(CVE-2023-4813) glibc-minimal-langpack@2.28-211.el8"
